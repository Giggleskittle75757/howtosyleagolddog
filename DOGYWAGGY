local function noplayer()
local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()



local t = mouse.Target
    local yarget = nil
    if t then
      for i, v in pairs(game.Workspace.vehicles:GetDescendants()) do
        if v.Name == "main" then
       
        
            if t:IsDescendantOf(v.Parent) then
                yarget = v
                break
           end
           
        end
      end
    end
    if yarget then --your code down here:
    
     function check()
     if game.Players.LocalPlayer.Character:FindFirstChild("FixedTP") == nil then
     
     
local pt = Instance.new("Part")
 pt.Parent = plr.Character
pt.Anchored = true
pt.CanCollide= false
pt.Name = "FixedTP"
pt.CFrame = plr.Character.HumanoidRootPart.CFrame
pt.Transparency = 1
     
     
     end
     end
    check()
    
  game.Players.LocalPlayer.Backpack["PvP Gun"].Parent = game.Players.LocalPlayer.Character



game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = yarget.CFrame + Vector3.new(1, 4, 0)



function kill()



game.Players.LocalPlayer.Character:WaitForChild("FixedTP")
if yarget.Parent == nil then
game.Workspace.Name = "Workspace"

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame

game.Players.LocalPlayer.Character.FixedTP:Destroy()
game.Players.LocalPlayer.Character["PvP Gun"].Parent = game.Players.LocalPlayer.Backpack
game.Players.LocalPlayer.Character["Click Explode Cars"].Parent = game.Players.LocalPlayer.Backpack
game.Players.LocalPlayer.Backpack["Click Explode Cars"].Parent = game.Players.LocalPlayer.Character


else




local args = {
[1] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
[2] = yarget.Position,
[3] = yarget.Parent,
[4] = yarget,
[5] = yarget.CFrame, 
[6] = game:GetService("Players").LocalPlayer.Character["PvP Gun"]
}
game:GetService("ReplicatedStorage").remoteInterface.jobs.hitVehicle:InvokeServer(unpack(args))
    
    
    
    
    
    
   end
    end


kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()



kill()

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame

game.Players.LocalPlayer.Character.FixedTP:Destroy()
game.Players.LocalPlayer.Character["PvP Gun"].Parent = game.Players.LocalPlayer.Backpack
game.Players.LocalPlayer.Character["Click Explode Cars"].Parent = game.Players.LocalPlayer.Backpack
game.Players.LocalPlayer.Backpack["Click Explode Cars"].Parent = game.Players.LocalPlayer.Character


    end

end



local function player1()
    local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()
    
    local t = mouse.Target
    local target = nil
    if t then
      for i, v in pairs(game.Players:GetPlayers()) do
        if v.Character and v.Character:FindFirstChild("vehicle") then
            if t:IsDescendantOf(v.Character) then
                target = v
                break
            end
        end
      end
    end
    if target then --your code down here:


     function check()
     if game.Players.LocalPlayer.Character:FindFirstChild("FixedTP") == nil then
     
     
local pt = Instance.new("Part")
 pt.Parent = plr.Character
pt.Anchored = true
pt.CanCollide= false
