        MainSection:NewButton("Click TP", "TPs you where you click", function()
            local plr = game.Players.LocalPlayer
            local mouse = plr:GetMouse()
            local tool = Instance.new("Tool", plr.Backpack)
            tool.Name = "Click TP"
            tool.CanBeDropped = false
            tool.RequiresHandle = false
            tool.Activated:connect(function()
        
        
        local pt = Instance.new("Part")
         pt.Parent = plr.Character
        pt.Anchored = true
        pt.CanCollide= false
        pt.Name = "FixedTP"
        pt.CFrame = plr.Character.HumanoidRootPart.CFrame
        
        local pos = mouse.Hit+Vector3.new(0, 3, 0)
                    pos = Vector3.new(pos.X, pos.Y, pos.Z)
        game.Players.LocalPlayer.Character.FixedTP.Position = pos
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
        game.Players.LocalPlayer.Character.FixedTP:Destroy()
        end)
        end)



