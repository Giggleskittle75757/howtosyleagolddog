    end
    if game.Players.LocalPlayer.Character:FindFirstChild("FixedTP") then
        game.Players.LocalPlayer.Character.FixedTP:Destroy()
    end
    local plr =  game.Players.LocalPlayer
    local pt = Instance.new("Part")
     pt.Parent = plr.Character
    pt.Anchored = true
    pt.CanCollide= false
    pt.Name = "FixedTP"
    pt.CFrame = plr.Character.HumanoidRootPart.CFrame
    pt.Transparency = 1
    
    
    
    local args = {
            [1] = "PickingTools",
            [2] = "Couch"
    
           
           
           }
            game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
            
            game.Players.LocalPlayer.Backpack.Couch.Parent = game.Players.LocalPlayer.Character
           
            game.Players.LocalPlayer.Character.Couch.Grip = game.Players.LocalPlayer.Character.Couch.Grip + Vector3.new(3, 4, 0)
           
           
           
           
           loop()




end
end)



loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()









