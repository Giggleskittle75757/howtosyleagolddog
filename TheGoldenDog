MapSection:NewLabel("Airport")


MapSection:NewButton("Send Noty", "", function()
    

    function check1()
    if game.Players.LocalPlayer.Character:FindFirstChild("Glock") == nil and
    game.Players.LocalPlayer.Backpack:FindFirstChild("Glock") == nil then
   
        local args = {
        [1] = "PickingTools",
        [2] = "Glock"

       
       
       }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))       

        game.Players.LocalPlayer.Backpack:WaitForChild("Glock")
        game.Players.LocalPlayer.Backpack.Glock.Parent = game.Players.LocalPlayer.Character

    end
end

function check2()
    if game.Players.LocalPlayer.Character:FindFirstChild("Glock") == nil then
   
         

        game.Players.LocalPlayer.Backpack:WaitForChild("Glock")
        game.Players.LocalPlayer.Backpack.Glock.Parent = game.Players.LocalPlayer.Character

    end
end
   
check1()
check2()


wait(0.7)

local plr =  game.Players.LocalPlayer
local pt = Instance.new("Part")
 pt.Parent = plr.Character
pt.Anchored = true
pt.CanCollide= false
pt.Name = "FixedTP"
pt.CFrame = plr.Character.HumanoidRootPart.CFrame
pt.Transparency = 1



plr.Character.HumanoidRootPart.CFrame = CFrame.new(315.085388, 5.31860304, 45.481945, 0.998467743, -2.25364101e-08, -0.0553365387, 2.13533742e-08, 1, -2.19701999e-08, 0.0553365387, 2.0754916e-08, 0.998467743)

wait(0.7)


game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame

game.Players.LocalPlayer.Character.FixedTP:Destroy()
 

end)






