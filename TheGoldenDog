local function SDSDY_fake_script() -- Aura.LocalScript 
	local script = Instance.new('LocalScript', Aura)

	script.Parent.MouseButton1Down:connect(function()
		
		-- Script generated by SimpleSpy - credits to exx#9394
	
		-- Script generated by SimpleSpy - credits to exx#9394
	
		local args = {
			[1] = "C4",
			[2] = false
		}
	
		game:GetService("ReplicatedStorage").ScriptPackages.Knit.Services.EquipablesService.RE.RequestToolByName:FireServer(unpack(args))
	
		game.Players.LocalPlayer.Backpack:WaitForChild("C4")
	
		game.Players.LocalPlayer.Backpack.C4.Parent = game.Players.LocalPlayer.Character
		game.Players.LocalPlayer.Character:WaitForChild("C4")
	
		game.Players.LocalPlayer.Character.C4.C4.CanCollide = true
		game.Players.LocalPlayer.Character.C4.Handle:Destroy()
	
		wait(0.2)
	
		_G.Ex = true
		while _G.Ex == true do
			wait()
	
			local args = {
				[1] = game:GetService("Players").LocalPlayer.Character.C4,
				[2] = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame,
				[3] = workspace._Plots:FindFirstChild("10").Building.Party_Mansion.Colors:FindFirstChild("2").Part
			}
	
			game:GetService("ReplicatedStorage").ScriptPackages.Knit.Services.EquipablesService.RE.ActivateTool:FireServer(unpack(args))
	
	
			-- Script generated by SimpleSpy - credits to exx#9394
	
			game:GetService("ReplicatedStorage").ScriptPackages.Knit.Services.EquipablesService.RE.RequestDetonateC4:FireServer()
		end
		
	end)
end
