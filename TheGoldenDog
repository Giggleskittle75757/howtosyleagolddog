-- Scripts:

local function MNNJKBL_fake_script() -- LoopsOFF.LocalScript 
	local script = Instance.new('LocalScript', LoopsOFF)

	script.Parent.MouseButton1Down:connect(function()
		
		_G.force = false
		while _G.force == true do
			wait()
			end
		
		
	end)
end
coroutine.wrap(MNNJKBL_fake_script)()
local function CZLHPS_fake_script() -- Kill.LocalScript 
	local script = Instance.new('LocalScript', Kill)

	script.Parent.MouseButton1Down:connect(function()
		
		
		workspace.FallenPartsDestroyHeight = -9999
		_G.force = true
		while _G.force == true do
			wait()
	
	
			if debounce == nil then
				if game.Players.LocalPlayer.Character:FindFirstChild("Shopping Cart") then
					if game.Players.LocalPlayer.Character["Shopping Cart"].Seat:FindFirstChild("SeatWeld") then
						local debounce = true
	
						local E = Instance.new("Part")
						E.Anchored = true
						E.Transparency = 1
						E.CanCollide = false
						E.Parent = workspace
						E.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
						E.Name = "tppart"
	
	
	
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, -999, 0)
						wait(0.0000000000001)
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
						wait(0.2)
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
						wait(0.3)
						game.Players.LocalPlayer.Character["Shopping Cart"].Parent = game.Players.LocalPlayer.Backpack
						wait(0.1)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tppart.CFrame
						wait(0.3)
						workspace.tppart:Destroy()
						if game.Players.LocalPlayer.Character:FindFirstChild("Shopping Cart") then
							if game.Players.LocalPlayer.Character["Shopping Cart"].Seat:FindFirstChild("SeatWeld") == nil then
							end
						end
					end
				end
			end
		end
		
	end)
end
coroutine.wrap(CZLHPS_fake_script)()
local function EPCQF_fake_script() -- VoidScreen.LocalScript 
	local script = Instance.new('LocalScript', VoidScreen)

	script.Parent.MouseButton1Down:connect(function()
		
	
		_G.force = true
		while _G.force == true do
			wait()
	
	
			if debounce == nil then
				if game.Players.LocalPlayer.Character:FindFirstChild("Shopping Cart") then
					if game.Players.LocalPlayer.Character["Shopping Cart"].Seat:FindFirstChild("SeatWeld") then
						local debounce = true
	
						local E = Instance.new("Part")
						E.Anchored = true
						E.Transparency = 1
						E.CanCollide = false
						E.Parent = workspace
						E.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
						E.Name = "tppart"
	
	
	
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9999999999999, 999999999999, 999999999999)
						wait(0.0000000000001)
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
	
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
						wait(0.7)
						game.Players.LocalPlayer.Character["Shopping Cart"].Parent = game.Players.LocalPlayer.Backpack
	
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tppart.CFrame
						wait(0.3)
						workspace.tppart:Destroy()
						if game.Players.LocalPlayer.Character:FindFirstChild("Shopping Cart") then
							if game.Players.LocalPlayer.Character["Shopping Cart"].Seat:FindFirstChild("SeatWeld") == nil then
							end
						end
					end
				end
			end
		end
		
		
	end)
end
