        game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)

AvatarSection:NewLabel("Character Appearance")




AvatarSection:NewButton("Fire Avatar", "", function()
        
    for i, v in pairs(game.Workspace:GetDescendants()) do
        if v.Name == "CatchFire" then
        if v.Parent.Name == "001_BBQ" then
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.0000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
        wait(0.000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.0000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
        
        end
        end
        end 
end)







AvatarSection:NewToggle("Loop Fire Avatar", "", function(state)
    if state then
        _G.fire = true
        while _G.fire == true do
            wait(0.3)
        
        for i, v in pairs(game.Workspace:GetDescendants()) do
        if v.Name == "CatchFire" then
        if v.Parent.Name == "001_BBQ" then
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.0000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
        wait(0.000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        
        
        end
        end
        end
        end
    else
        _G.fire = false  


    end
end)





AvatarSection:NewButton("Smoke Avatar", "", function()
    for i, v in pairs(game.Workspace:GetDescendants()) do
        if v.Name == "CatchFire" then
        if v.Parent == game.Workspace.WorkspaceCom["001_Mall"]["001_Pizza"] then
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.0000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
        wait(0.000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.0000000000000000000000001)
        v.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
        
        end
        end
        end
end)





