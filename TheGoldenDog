local Abilities = Window:NewTab("Abilities")
local AbilitiesSection = Abilities:NewSection("Click Tools")


AbilitiesSection:NewButton("Click Kill", "", function()
    local plr = game.Players.LocalPlayer
    local mouse = plr:GetMouse()
    local tool = Instance.new("Tool", plr.Backpack)
    tool.Name = "Click Kill"
    tool.CanBeDropped = false
    tool.RequiresHandle = false
    
    tool.Activated:connect(function()
        local t = mouse.Target
        local target = nil
        if t then
          for i, v in pairs(game.Players:GetPlayers()) do
            if v.Character and v.Character:FindFirstChild("UpperTorso") or
            v.Character and v.Character:FindFirstChild("LowerTorso") or 
            v.Character and v.Character:FindFirstChild("Head") or
            v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
            
                if t:IsDescendantOf(v.Character) then
                    target = v
                    break
                end
            end
          end
        end
        if target then --your code down here:
    
    
    
    
    
    function loop()
    _G.couch = true
    while _G.couch == true do
        wait()
    
    if game.Players.LocalPlayer.Character.Couch.Seat1:FindFirstChild("SeatWeld") == nil and
    game.Players.LocalPlayer.Character.Couch.Seat2:FindFirstChild("SeatWeld") == nil then
    
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(2, -3, 6)
    wait(0.0000000000000001)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(3, -3, 0)
    wait(0.0000000000001)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-2, -3, -10)
    wait(0.0000000000000001)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-0, -3, 0)
    wait(0.0000000000000001)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(10, -3, -4)
    wait(0.0000000000000001)
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-0, -3, 0)
    wait(0.0000000000000001)
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-10, -3, 4)
    wait(0.0000000000000001)
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-10, -3, 2)
     wait(0.0000000000000001)
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(-0, -3, 0)
    
    else
    
    _G.couch = false
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, -40000, 1)
    wait(0.4)
     local args = {
            [1] = "ClearAllTools",
       
    [2] = "Couch"
           
           
           }
            game:GetService("ReplicatedStorage").RE:FindFirstChild("1Clea1rTool1s"):FireServer(unpack(args))
    
    wait(0.3)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
    
    game.Players.LocalPlayer.Character.FixedTP:Destroy()
    
    game.Players.LocalPlayer.Character["Click Kill"].Parent = game.Players.LocalPlayer.Backpack
    game.Players.LocalPlayer.Backpack["Click Kill"].Parent = game.Players.LocalPlayer.Character
    
    end
    end
    end
    workspace.FallenPartsDestroyHeight = -50000
    local plr =  game.Players.LocalPlayer
    local pt = Instance.new("Part")
     pt.Parent = plr.Character
    pt.Anchored = true
    pt.CanCollide= false
    pt.Name = "FixedTP"
    pt.CFrame = plr.Character.HumanoidRootPart.CFrame
    pt.Transparency = 1
    
    
    
    local args = {
            [1] = "PickingTools",
            [2] = "Couch"
    
           
           
           }
            game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
            
            game.Players.LocalPlayer.Backpack.Couch.Parent = game.Players.LocalPlayer.Character
           
            game.Players.LocalPlayer.Character.Couch.Grip = game.Players.LocalPlayer.Character.Couch.Grip + Vector3.new(3, 4, 0)
           
           
           
           
           loop()
            
          end
          end)
            
end)



