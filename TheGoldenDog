    HouseSection:NewButton("Unban", "", function()
        --while task.wait() do

for i, v in pairs(workspace:GetDescendants()) do
    if v.ClassName == "Part" then
     if v.BrickColor == BrickColor.new("Really red") then
     if v.Size == Vector3.new(92.30000305175781, 340, 104.99984741210938) then
     
     
     
     
     v:Destroy()
    end
    end
    end
    end
    --end
    end)

    
    HouseSection:NewToggle("Auto Unban", "", function(state)
        if state then
            _G.unb = true
